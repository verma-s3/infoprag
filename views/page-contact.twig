{% import './components/image.twig' as Image %}
{% import './components/button.twig' as Button %}
{% extends "base.twig" %}

{% block content %}
	<section class="{{field.slug}}-page padding" id="{{field.slug}}-page">
		<div class="container">
			<div class="text-content">
				{% set page = field.meta('contact') %}
				<h2 class="h3 title">{{page.title}}</h2>
				<div class="content">{{page.content}}</div>
			</div>
			<div class="form-container">
				{{fn('do_shortcode','[gravityform id="1" title="false" description="false" ajax="true"]')}}
			</div>
		</div>
	</section>

{% endblock %}
